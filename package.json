{
	"name": "nix",
	"displayName": "Nix",
	"description": "Nix language support for Visual Studio Code.",
	"version": "1.0.1",
	"publisher": "bbenoist",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#FFFFFF",
		"theme": "light"
	},
	"license": "SEE LICENSE IN LICENSE.md",
	"bugs": {
		"url": "https://github.com/bbenoist/vscode-nix/issues"
	},
	"homepage": "https://github.com/bbenoist/vscode-nix/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/bbenoist/vscode-nix.git"
	},
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "nix",
				"aliases": ["Nix"],
				"extensions": [
					".nix"
				],
				"configuration": "./nix.language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "nix",
				"scopeName": "source.nix",
				"path": "./syntaxes/nix.tmLanguage"
			},
			{
				"path": "./syntaxes/codeblock.tmLanguage",
				"scopeName": "markdown.nix.codeblock",
				"injectTo": ["text.html.markdown"],
				"embeddedLanguages": {
					"meta.embedded.block.nix": "nix"
				}
			}
		]
	},
	"__metadata": {
		"id": "5b21bd54-5258-4a3e-887f-193d9bb5f063",
		"publisherDisplayName": "Baptist BENOIST",
		"publisherId": "787ce942-60d4-48d6-b14e-25bdc72bd145"
	}
}
